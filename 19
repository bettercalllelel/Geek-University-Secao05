num = int(input('Escolha um número:\n'))

if num%3==0 or num%5==0:
    if num%3==0 and num%5==0:
        print('É divisível por ambos.')

    else:
        if num%3==0:
            print('É divisível apenas por 3.')

        else:
            print('É divisível apenas por 5.')

else:
    print('Não é divisível por 3 ou por 5.')
