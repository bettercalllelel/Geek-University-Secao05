a = int(input('Diga o primeiro comprimento do triângulo:\n'))
b = int(input('Diga o segundo comprimento do triângulo:\n'))
c = int(input('Diga o primeiro comprimento do triângulo:\n'))

if a == b or a == c or b == c:
    if a == b == c:
        print('É um triângulo equilátero!')

    else:
        print('É um triângulo isósceles!')

elif a <= 0 or b <= 0 or c <= 0:
    print('Não é um triângulo!')

else:
    print('É um triângulo escaleno!')
